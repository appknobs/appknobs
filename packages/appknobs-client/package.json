{
  "name": "@appknobs/client",
  "description": "Appknobs.io client for multiple environments",
  "version": "0.5.0",
  "license": "Apache-2.0",
  "homepage": "http://appknobs.io",
  "repository": "https://github.com/appknobs/appknobs",
  "bugs": {
    "url": "https://github.com/appknobs/appknobs/issues"
  },
  "keywords": [
    "Appknobs.io",
    "client",
    "feature-flags",
    "feature-toggles",
    "feature-switch"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "test": "jest src/",
    "tdd": "jest src/ --watch",
    "build": "rm -rf lib && tsc",
    "build-lib": "yarn build",
    "watch": "tsc --watch",
    "watch-lib": "yarn watch",
    "prepack": "yarn build"
  },
  "dependencies": {
    "cross-fetch": "^3.0.1",
    "electron-fetch": "^1.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest-date-mock": "^1.0.7"
  },
  "jest": {
    "setupFiles": [
      "jest-date-mock"
    ]
  }
