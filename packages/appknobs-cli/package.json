{
  "name": "@appknobs/cli",
  "description": "CLI interface for Appknobs.io",
  "version": "0.4.7",
  "license": "Apache-2.0",
  "homepage": "http://appknobs.io",
  "keywords": [
    "Appknobs.io",
    "cli",
    "automation",
    "feature-flags",
    "feature-toggles",
    "feature-switch"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "files": [
    "lib/*"
  ],
  "bin": {
    "appknobs": "./lib/cli.js",
    "knobs": "./lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && yarn stampVersion",
    "stampVersion": "sed \"s/VERSION/$npm_package_version/g\" lib/version.js > version.js && mv version.js lib/version.js",
    "build-lib": "yarn build",
    "watch": "tsc --watch",
    "watch-lib": "yarn watch",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@appknobs/cognito": "^0.4.0",
    "@appknobs/react-parser": "^0.4.0",
    "@appknobs/services-client": "^0.4.6",
    "cfonts": "^2.4.2",
    "commander": "^2.17.1",
    "cosmiconfig": "^5.0.6",
    "debug": "^4.0.1",
    "email-validator": "^2.0.4",
    "enquirer": "^2.3.0",
    "find-up": "^3.0.0",
    "mkdirp": "^0.5.1",
    "opn": "^5.4.0",
    "ora": "^3.0.0"
  },
  "devDependencies": {
    "@types/ora": "^1.3.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
